{
  "functions": {
    "api/data.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "POST, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, X-Coinglass-Extension" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/data", "destination": "/api/data" }
  ]
}
